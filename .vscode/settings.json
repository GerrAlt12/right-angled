// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.d.ts": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": {
            "when": "$(basename)"
        },
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.metadata.json": true
    },
    "search.exclude": {
        "**/docs": true,
        "**/compiled": true,
        "**/coverage": true
    }
,
"typescript.tsdk": "./node_modules/typescript/lib"
}